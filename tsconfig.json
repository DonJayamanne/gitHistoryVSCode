{
    "compilerOptions": {
        "lib": ["dom","es6"],
        "module": "commonjs",
        "outDir": "out",
        "rootDir": ".",
        "sourceMap": true,
        "target": "es6",

        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "strictNullChecks": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "exclude": [
        "node_modules",
        ".vscode-test"
    ]
}